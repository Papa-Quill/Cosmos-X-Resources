"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\w_rocketlauncher/w_rocketlauncher01"
	
	"$phong" "1"
	"$phongexponent" "85"
	"$phongboost" "5"	
	//"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[2.25 2.5 20]"
	"$halflambert" "1"

	"$glowcolor" "1"

	// Begin GOLD look

	"$envmap" "env_cubemap"	
	"$envmaptint" "[1.92 1.61 0.38]"
	
	// End GOLD look
	
	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"					
	"$rimlightexponent" "4"		
	"$rimlightboost" "2"

	"360?$color2" "[ 0.8 0.8 0.8 ]"

	// Cloaking
	"$cloakPassEnabled" "1"
	
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}